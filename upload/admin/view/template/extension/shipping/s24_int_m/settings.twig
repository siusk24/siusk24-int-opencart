                        <input type="text" name="option_name" id="sm-option-name" class="form-control" placeholder="{{ s24_int_m_generic_placeholder_type_in }}">
                    </div>

                    <label class="col-sm-2 control-label">{{ s24_int_m_label_sm_modal_type }}</label>
                    <div class="col-sm-4">
                        {% for key, option in shipping_types %}
                        <label class="radio-inline">
                            <div class="price-priority-wrapper">
                                <input type="radio" name="option_type" value="{{ key }}">{{ option }}
                            </div>
                        </label>
                        {% endfor %}
                    </div>
                </div>

                <div class="form-group sm-modal-global-price">
                    <label class="col-sm-2 control-label">{{ s24_int_m_label_sm_modal_price_type }}</label>
                    <div class="col-sm-4">
                        <select name="option_price_type" class="form-control"
                            data-price-type-selector-addon="#shipping_option_modal [data-global-price-addon]"
                        >
                            {% for key, option in price_types %}
                                {% if key != 0 %}
                                    <option value="{{ key }}">{{ option }}</option>
                                {% endif %}
                            {% endfor %}
                        </select>
                    </div>
                    <label class="col-sm-2 control-label">{{ s24_int_m_label_sm_modal_price_type_value }}</label>
                    <div class="col-sm-4">
                        <div class="input-group">
                            <input type="text" name="option_price" class="form-control" placeholder="{{ s24_int_m_generic_placeholder_type_in }}">
                            <span class="input-group-addon option_price" data-global-price-addon>&euro;</span>
                        </div>
                    </div>
                </div>

                <div class="form-group sm-modal-global-price">
                    <label class="col-sm-2 control-label">{{ s24_int_m_label_sm_modal_free_shipping }}</label>
                    <div class="col-sm-4">
                        <div class="input-group">
                            <input type="text" name="option_free_shipping" class="form-control" placeholder="{{ s24_int_m_generic_placeholder_type_in }}">
                            <span class="input-group-addon">&euro;</span>
                        </div>
                    </div>
                    <label class="col-sm-2 control-label">{{ s24_int_m_label_sm_modal_offer_priority }}</label>
                    <div class="col-sm-4">
                        {% for key, option in priority_types %}
                            {% if key != 0 %}
                                <label class="radio-inline">
                                    <div class="price-priority-wrapper">
                                        <input type="radio" name="option_price_priority" value="{{ key }}">{{ option }}
                                    </div>
                                </label>
                            {% endif %}
                        {% endfor %}
                    </div>
                </div>

                <div class="form-group sm-modal-services-grid">
                    <div class="col-sm-12">
                        <div class="services-grid">
                            <div class="available-services">
                                <label>{{ s24_int_m_label_sm_modal_available_services }}</label>
                                <select multiple name="available_services" class="form-control">
                                </select>
                            </div>
                            <div class="service-action">
                                <button class="btn btn-s24 btn-s24-info" 
                                    data-select-service-btn data-type="add" data-from="[name='available_services']" data-to="[name='selected_services']"
                                >
                                    <svg width="17" height="12" viewBox="0 0 17 12" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path d="M9.80244 11.565C9.60244 11.365 9.50644 11.1233 9.51444 10.84C9.52311 10.5567 9.62744 10.315 9.82744 10.115L12.6524 7.29H1.50244C1.21911 7.29 0.981442 7.194 0.789442 7.002C0.598108 6.81067 0.502441 6.57333 0.502441 6.29C0.502441 6.00667 0.598108 5.769 0.789442 5.577C0.981442 5.38567 1.21911 5.29 1.50244 5.29H12.6524L9.80244 2.44C9.60244 2.24 9.50244 2.00233 9.50244 1.727C9.50244 1.45233 9.60244 1.215 9.80244 1.015C10.0024 0.815 10.2401 0.715 10.5154 0.715C10.7901 0.715 11.0274 0.815 11.2274 1.015L15.8024 5.59C15.9024 5.69 15.9734 5.79833 16.0154 5.915C16.0568 6.03167 16.0774 6.15667 16.0774 6.29C16.0774 6.42333 16.0568 6.54833 16.0154 6.665C15.9734 6.78167 15.9024 6.89 15.8024 6.99L11.2024 11.59C11.0191 11.7733 10.7901 11.865 10.5154 11.865C10.2401 11.865 10.0024 11.765 9.80244 11.565Z" fill="#979797"/>
                                    </svg>
                                </button>
                                <button class="btn btn-s24 btn-s24-info" 
                                    data-select-service-btn data-type="remove" data-from="[name='selected_services']" data-to="[name='available_services']"
                                >
                                    <svg width="17" height="12" viewBox="0 0 17 12" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path d="M6.77764 1.01496C6.97764 1.21496 7.07364 1.45662 7.06564 1.73996C7.05697 2.02329 6.95264 2.26496 6.75264 2.46496L3.92764 5.28996L15.0776 5.28996C15.361 5.28996 15.5986 5.38596 15.7906 5.57796C15.982 5.76929 16.0776 6.00662 16.0776 6.28996C16.0776 6.57329 15.982 6.81096 15.7906 7.00296C15.5986 7.19429 15.361 7.28996 15.0776 7.28996L3.92764 7.28996L6.77764 10.14C6.97764 10.34 7.07764 10.5776 7.07764 10.853C7.07764 11.1276 6.97764 11.365 6.77764 11.565C6.57764 11.765 6.33997 11.865 6.06464 11.865C5.78997 11.865 5.55264 11.765 5.35264 11.565L0.777637 6.98996C0.677637 6.88996 0.606636 6.78162 0.564636 6.66496C0.523303 6.54829 0.502635 6.42329 0.502635 6.28996C0.502635 6.15662 0.523303 6.03162 0.564636 5.91496C0.606636 5.79829 0.677637 5.68996 0.777637 5.58996L5.37764 0.989958C5.56097 0.806624 5.78997 0.714956 6.06464 0.714956C6.33997 0.714956 6.57764 0.814957 6.77764 1.01496Z" fill="#979797"/>
                                    </svg>
                                </button>
                            </div>
                            <div class="selected-services">
                                <label>{{ s24_int_m_label_sm_modal_selected_services }}</label>
                                <span class="selected-services-inner">
                                    <select multiple name="selected_services" class="form-control">
                                    </select>
                                    <div class="s24-no-services-selected">
                                        <h4>{{ s24_int_m_sm_modal_no_services_assigned_info_title }}</h4>
                                        <p>
                                            {{ s24_int_m_sm_modal_no_services_assigned_info_part_1 }}
                                            <button class="btn btn-s24 btn-s24-info" disabled>
                                                <svg width="17" height="12" viewBox="0 0 17 12" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                    <path d="M9.80244 11.565C9.60244 11.365 9.50644 11.1233 9.51444 10.84C9.52311 10.5567 9.62744 10.315 9.82744 10.115L12.6524 7.29H1.50244C1.21911 7.29 0.981442 7.194 0.789442 7.002C0.598108 6.81067 0.502441 6.57333 0.502441 6.29C0.502441 6.00667 0.598108 5.769 0.789442 5.577C0.981442 5.38567 1.21911 5.29 1.50244 5.29H12.6524L9.80244 2.44C9.60244 2.24 9.50244 2.00233 9.50244 1.727C9.50244 1.45233 9.60244 1.215 9.80244 1.015C10.0024 0.815 10.2401 0.715 10.5154 0.715C10.7901 0.715 11.0274 0.815 11.2274 1.015L15.8024 5.59C15.9024 5.69 15.9734 5.79833 16.0154 5.915C16.0568 6.03167 16.0774 6.15667 16.0774 6.29C16.0774 6.42333 16.0568 6.54833 16.0154 6.665C15.9734 6.78167 15.9024 6.89 15.8024 6.99L11.2024 11.59C11.0191 11.7733 10.7901 11.865 10.5154 11.865C10.2401 11.865 10.0024 11.765 9.80244 11.565Z" fill="#979797"/>
                                                </svg>
                                            </button>
                                            {{ s24_int_m_sm_modal_no_services_assigned_info_part_2 }}
                                        </p>
                                    </div>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="form-group sm-modal-api-countries">
                    <label class="control-label">{{ s24_int_m_label_sm_modal_country }}</label>
                    <div class="api-countries-select-wrapper">
                        <select name="api_countries" class="form-control" style="width: 100%;">
                            {% for option in countries %}
                            <option value="{{ option.get('code') }}">{{ option.get('en_name') }}</option>
                            {% endfor %}
                        </select>
                    </div>
                    <div class="">
                        <button class="btn btn-s24 btn-s24-primary sm-modal-add-country-btn">
                            <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <path d="M7.08346 15.3857V9.15917H0.856934V7.08366H7.08346V0.857132H9.15897V7.08366H15.3855V9.15917H9.15897V15.3857H7.08346Z" fill="white"/>
                            </svg>
                        </button>
                    </div>
                </div>

                <div class="form-group sm-modal-api-countries api-countries-table">
                    <div class="col-sm-12 table-responsive">
                        <table class="table table-striped table-hover">
                            <thead>
                                <tr>
                                    <th>{{ s24_int_m_sm_table_country }}</th>
                                    <th>{{ s24_int_m_sm_table_offer_priority }}</th>
                                    <th colspan="2">{{ s24_int_m_sm_table_price }}</th>
                                    <th colspan="2">{{ s24_int_m_sm_table_free_shipping }}</th>
                                    {# <th>{{ s24_int_m_sm_table_actions }}</th> #}
                                </tr>
                            </thead>
                            <tbody id="sm-modal-countries">
                                <tr data-placeholder data-modal-no-country>
                                    <td colspan="5">{{ s24_int_m_sm_table_no_country }}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>

                <div class="form-group s24-modal-sm-buttons">
                    <button class="btn btn-s24 btn-s24-info" data-btn-cancel-add-shipping-method>{{ s24_int_m_generic_btn_discard }}</button>
                    <button class="btn btn-s24 btn-s24-primary" data-btn-save-add-shipping-method>
                        <span data-modal-type="create">{{ s24_int_m_generic_btn_create }}</span>
                        <span data-modal-type="edit">{{ s24_int_m_generic_btn_update }}</span>
                    </button>
                </div>
            </div>
        </div>
    </div>
    
    <!-- Parcel Defaults Edit Modal -->
    <div class="s24-int-m-modal hidden" id="parcel_default_modal">
