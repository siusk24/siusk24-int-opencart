                <div class="form-group">
                    <label class="col-sm-2 control-label" for="modal-pd-hs-code">{{ s24_int_m_label_pd_hs_code }}</label>
                    <div class="col-sm-10">
                        <input type="text" name="s24_int_m_pd_hs_code" value="{{ global_parcel_default.hs_code }}" id="modal-pd-hs-code" class="form-control" />
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-sm-2 control-label" for="modal-pd-weight">{{ s24_int_m_label_pd_weight }}</label>
                    <div class="col-sm-10">
                        <div class="input-group">
                            <input type="text" name="s24_int_m_pd_weight" value="{{ global_parcel_default.weight }}" id="modal-pd-weight" class="form-control" />
                            <span class="input-group-addon">kg</span>
                        </div>
                    </div>
                </div>

                <div class="form-group pd-dimensions">
                    <label class="col-sm-2 control-label" for="modal-pd-length">{{ s24_int_m_label_pd_dimensions }}</label>
                    <div class="col-sm-10">
                        <div class="dimmension-wrapper">
                            <div class="dimmension-inner">
                                <div class="input-group">
                                    <input type="text" name="s24_int_m_pd_length" value="{{ global_parcel_default.length }}" id="modal-pd-length" class="form-control" />
                                    <span class="input-group-addon">cm</span>
                                </div>
                                <span class="dimmension-input-label">{{ s24_int_m_label_pd_length }}</span>
                            </div>

                            <div class="dimmension-inner">
                                <div class="input-group">
                                    <input type="text" name="s24_int_m_pd_width" value="{{ global_parcel_default.width }}" id="modal-pd-width" class="form-control" />
                                    <span class="input-group-addon">cm</span>
                                </div>
                                <span class="dimmension-input-label">{{ s24_int_m_label_pd_width }}</span>
                            </div>

                            <div class="dimmension-inner">
                                <div class="input-group">
                                    <input type="text" name="s24_int_m_pd_height" value="{{ global_parcel_default.height }}" id="modal-pd-height" class="form-control" />
                                    <span class="input-group-addon">cm</span>
                                </div>
                                <span class="dimmension-input-label">{{ s24_int_m_label_pd_height }}</span>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="form-group s24-modal-sm-buttons">
                    <button class="btn btn-s24 btn-s24-info" data-btn-cancel-parcel-default>
                        {{ s24_int_m_generic_btn_discard }}
                    </button>
                    <button class="btn btn-s24 btn-s24-primary" data-btn-save-parcel-default>
