{{ header }}
{{ column_left }}

<div id="content" class="s24_int_m_content loading">
    <div class="page-header">
        <div class="container-fluid">
            <div class="s24-module-header-wrapper">
                <h1><img src="view/image/s24_int_m/s24_logo.svg" alt="Siusk24.lt Logo" style="height: 33px;"></h1>
                <ul class="breadcrumb">
                    {% for breadcrumb in breadcrumbs %}
                        <li><a href="{{ breadcrumb['href'] }}">{{ breadcrumb['text'] }}</a></li>
                    {% endfor %}
                </ul>
                <span class="s24-version">v{{ s24_int_m_version }}</span>
            </div>
        </div>
    </div>

    <!-- Errors / Success -->
    <div class="container-fluid">
        {% if error_warning %}
            <div class="alert alert-danger"><i class="fa fa-exclamation-circle"></i>{{ s24_int_m_problems_found }}
                <button type="button" class="close" data-dismiss="alert">&times;</button>
                <ul>
                    {% for error_string in error_warning %}
                        <li>{{ error_string }}</li>
                    {% endfor %}
                </ul>
            </div>
        {% endif %}
        {% if success %}
            <div class="alert alert-success"><i class="fa fa-exclamation-circle"></i>
                <button type="button" class="close" data-dismiss="alert">&times;</button>
                {{ success }}
            </div>
        {% endif %}
    </div>

    <!-- VERSION CHECK -->
    {% if s24_int_m_git_version %}
        <div class="container-fluid">
            <div class="alert alert-warning"><i class="fa fa-exclamation-circle"></i>{{ s24_int_m_new_version_notify|replace({'$$s24_int_m_new_version$$': s24_int_m_git_version.version}) }} 
                <a href="{{ s24_int_m_git_version.download_url }}" target="_blank" class="btn btn-success">{{ s24_int_m_button_download_version }}</a>
            </div>
        </div>
    {% endif %}

    <!-- DB CHECK -->
    {% if s24_int_m_db_check %}
        <div class="container-fluid">
            <div class="alert alert-warning"><i class="fa fa-exclamation-circle"></i>{{ s24_int_m_db_fix_notify }} 
                <a href="{{ s24_int_m_db_fix_url }}" class="btn btn-success">{{ s24_int_m_button_fix_db }}</a>
            </div>
        </div>
    {% endif %}

    <!-- XML CHECK -->
    {% if s24_int_m_xml_check %}
        <div class="container-fluid">
            <div class="alert alert-warning"><i class="fa fa-exclamation-circle"></i>{{ s24_int_m_xml_fix_notify }} 
                <a href="{{ s24_int_m_xml_fix_url }}" class="btn btn-success">{{ s24_int_m_button_fix_xml }}</a>
            </div>
        </div>
    {% endif %}

    <ul class="container-fluid nav nav-tabs">
        <li class="active"><a href="#tab-general" data-toggle="tab">{{ s24_int_m_tab_general }}</a></li>
        <li><a href="#tab-shipping-method" data-toggle="tab">{{ s24_int_m_tab_shipping_method }}</a></li>
        <li><a href="#tab-services" data-toggle="tab" {% if not s24_int_m_api_test_mode %}style="display: none;"{% endif %}>{{ s24_int_m_tab_services }}</a></li>
        <li><a href="#tab-sender-info" data-toggle="tab">{{ s24_int_m_tab_sender_info }}</a></li>
        <li><a href="#tab-parcel-defaults" data-toggle="tab">{{ s24_int_m_tab_parcel_defaults }}</a></li>
    </ul>

    <div class="tab-content">
        <!-- Module/API Settings -->
        <div class="tab-pane active" id="tab-general">
            <form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-s24_int_m" class="form-horizontal">
                <input type="hidden" name="module_settings_update">

                <div class="container-fluid">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title">
                                <svg width="19" height="19" viewBox="0 0 19 19" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path d="M13.4375 4.9375H16.25M2.75 4.9375H11.1875M13.4375 13.9375H16.25M2.75 13.9375H11.1875M7.8125 9.4375H16.25M2.75 9.4375H5.5625" stroke="#4C4D5A" stroke-width="1.125" stroke-linecap="round" stroke-linejoin="round"/>
                                    <path d="M12.3125 6.0625C12.9338 6.0625 13.4375 5.55882 13.4375 4.9375C13.4375 4.31618 12.9338 3.8125 12.3125 3.8125C11.6912 3.8125 11.1875 4.31618 11.1875 4.9375C11.1875 5.55882 11.6912 6.0625 12.3125 6.0625Z" stroke="#4C4D5A" stroke-width="1.125" stroke-linecap="round" stroke-linejoin="round"/>
                                    <path d="M6.6875 10.5625C7.30882 10.5625 7.8125 10.0588 7.8125 9.4375C7.8125 8.81618 7.30882 8.3125 6.6875 8.3125C6.06618 8.3125 5.5625 8.81618 5.5625 9.4375C5.5625 10.0588 6.06618 10.5625 6.6875 10.5625Z" stroke="#4C4D5A" stroke-width="1.125" stroke-linecap="round" stroke-linejoin="round"/>
                                    <path d="M12.3125 15.0625C12.9338 15.0625 13.4375 14.5588 13.4375 13.9375C13.4375 13.3162 12.9338 12.8125 12.3125 12.8125C11.6912 12.8125 11.1875 13.3162 11.1875 13.9375C11.1875 14.5588 11.6912 15.0625 12.3125 15.0625Z" stroke="#4C4D5A" stroke-width="1.125" stroke-linecap="round" stroke-linejoin="round"/>
                                </svg>
                                {{ s24_int_m_title_edit }}
                            </h3>
                        </div>

                        <div class="panel-body">
                            <div class="form-group">
                                <label class="col-sm-2 control-label" for="input-status">{{ s24_int_m_label_status }}</label>
                                <div class="col-sm-6">
                                    <div class="checkbox checbox-switch switch-primary">
                                        <label>
                                            <input type="checkbox" name="s24_int_m_status" id="input-status" {% if s24_int_m_status %}checked{% endif %}/>
                                            <span></span>
                                            <div data-status="on">{{ s24_int_m_generic_enabled }}</div>
                                            <div data-status="off">{{ s24_int_m_generic_disabled }}</div>
                                        </label>
                                    </div>
                                    <p class="help-block">{{ s24_int_m_help_status }}</p>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-sm-2 control-label" for="input-tax-class">{{ s24_int_m_label_tax_class }}</label>
                                <div class="col-sm-6">
                                    <select name="s24_int_m_tax_class_id" id="input-tax-class" class="form-control">
                                        <option value="0">{{ text_none }}</option>
                                        {% for tax_class in tax_classes %}
                                            {% if tax_class['tax_class_id'] == s24_int_m_tax_class_id %}
                                                <option value="{{ tax_class['tax_class_id'] }}" selected="selected">{{ tax_class['title'] }}</option>
                                            {% else %}
                                                <option value="{{ tax_class['tax_class_id'] }}">{{ tax_class['title'] }}</option>
                                            {% endif %}
                                        {% endfor %}
                                    </select>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-sm-2 control-label" for="input-geo-zone">{{ s24_int_m_label_geo_zone }}</label>
                                <div class="col-sm-6">
                                <select name="s24_int_m_geo_zone_id" id="input-geo-zone" class="form-control">
                                    <option value="0">{{ text_all_zones }}</option>
                                    {% for geo_zone in geo_zones %}
                                    {% if geo_zone['geo_zone_id'] == s24_int_m_geo_zone_id %}
                                        <option value="{{ geo_zone['geo_zone_id'] }}" selected="selected">{{ geo_zone['name'] }}</option>
                                    {% else %}
                                        <option value="{{ geo_zone['geo_zone_id'] }}">{{ geo_zone['name'] }}</option>
                                    {% endif %}
                                    {% endfor %}
                                </select>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-sm-2 control-label" for="input-sort-order">{{ s24_int_m_label_sort_order }}</label>
                                <div class="col-sm-6">
                                    <input type="text" name="s24_int_m_sort_order" id="input-sort-order" class="form-control"
                                        value="{{ s24_int_m_sort_order }}"
                                        placeholder="{{ s24_int_m_generic_placeholder_type_in }}"
                                    />
                                    <p class="help-block">{{ s24_int_m_help_sort_order }}</p>
                                </div>
                            </div>
                        </div>
                    </div> <!-- General panel -->

                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title">
                                <svg width="19" height="18" viewBox="0 0 19 18" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path d="M16.25 2.25L14.375 4.125M9.125 12L10.625 10.5M2.75 15.75L4.625 13.875M6.5 9.375L8 7.875M14.375 8.25L13.25 9.375L9.125 5.25L10.25 4.125C10.8125 3.5625 12.875 2.625 14.375 4.125C15.875 5.625 14.9375 7.6875 14.375 8.25ZM4.625 9.75L5.75 8.625L9.875 12.75L8.75 13.875C8.1875 14.4375 6.125 15.375 4.625 13.875C3.125 12.375 4.0625 10.3125 4.625 9.75Z" stroke="#4C4D5A" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                                </svg>

                                {{ s24_int_m_title_api_settings }}
                            </h3>
                        </div>

                        <div class="panel-body">
                            <div class="form-group">
                                <label class="col-sm-2 control-label" for="input-api-token">{{ s24_int_m_label_api_token }}</label>
                                <div class="col-sm-6">
                                    <input type="text" name="s24_int_m_api_token" value="{{ s24_int_m_api_token }}" id="input-api-token" class="form-control" />
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-sm-2 control-label" for="input-api-test-mode">{{ s24_int_m_label_api_test_mode }}</label>
                                <div class="col-sm-6">
                                    <div class="checkbox checbox-switch switch-primary">
                                        <label>
                                            <input type="checkbox" name="s24_int_m_api_test_mode" id="input-api-test-mode" {% if s24_int_m_api_test_mode %}checked{% endif %}/>
                                            <span></span>
                                            <div data-status="on">{{ s24_int_m_generic_enabled }}</div>
                                            <div data-status="off">{{ s24_int_m_generic_disabled }}</div>
                                        </label>
                                    </div>
                                    <p class="help-block">{{ s24_int_m_help_test_mode }}</p>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-sm-2 control-label" for="input-api-consolidate">{{ s24_int_m_label_api_consolidate }}</label>
                                <div class="col-sm-6">
                                    <div class="checkbox checbox-switch switch-primary">
                                        <label>
                                            <input type="checkbox" name="s24_int_m_api_consolidate" id="input-api-consolidate" {% if s24_int_m_api_consolidate %}checked{% endif %}/>
                                            <span></span>
                                            <div data-status="on">{{ s24_int_m_generic_enabled }}</div>
                                            <div data-status="off">{{ s24_int_m_generic_disabled }}</div>
                                        </label>
                                    </div>
                                    <p class="help-block">{{ s24_int_m_help_consolidate }}</p>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-sm-2 control-label" for="input-api-use-vat-price">{{ s24_int_m_label_api_use_vat_price }}</label>
                                <div class="col-sm-6">
                                    <div class="checkbox checbox-switch switch-primary">
                                        <label>
                                            <input type="checkbox" name="s24_int_m_api_use_vat_price" id="input-api-use-vat-price" {% if s24_int_m_api_use_vat_price %}checked{% endif %}/>
                                            <span></span>
                                            <div data-status="on">{{ s24_int_m_generic_enabled }}</div>
                                            <div data-status="off">{{ s24_int_m_generic_disabled }}</div>
                                        </label>
                                    </div>
                                    <p class="help-block">{{ s24_int_m_help_use_vat_price }}</p>
                                </div>
                            </div>
                        </div>
                    </div> <!-- API panel -->
                </div> <!-- Container -->

                <div class="container-fluid">
                    <div class="clearfix">
                        <div class="pull-right">
                            <button type="submit" form="form-s24_int_m" class="btn btn-s24 btn-s24-primary">
                                <svg width="23" height="23" viewBox="0 0 23 23" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path d="M4.99788 20.1172C4.49372 20.1172 4.06197 19.9378 3.70263 19.5791C3.34391 19.2198 3.16455 18.788 3.16455 18.2839V5.45052C3.16455 4.94635 3.34391 4.5146 3.70263 4.15527C4.06197 3.79655 4.49372 3.61719 4.99788 3.61719H15.2416C15.4861 3.61719 15.7192 3.66302 15.9411 3.75469C16.1623 3.84635 16.3569 3.97622 16.525 4.14427L19.1375 6.75677C19.3055 6.92483 19.4354 7.11947 19.5271 7.34069C19.6187 7.56252 19.6646 7.79566 19.6646 8.0401V18.2839C19.6646 18.788 19.4852 19.2198 19.1265 19.5791C18.7671 19.9378 18.3354 20.1172 17.8312 20.1172H4.99788ZM17.8312 8.06302L15.2187 5.45052H4.99788V18.2839H17.8312V8.06302ZM11.4146 17.3672C12.1784 17.3672 12.8277 17.0998 13.3625 16.5651C13.8972 16.0304 14.1646 15.3811 14.1646 14.6172C14.1646 13.8533 13.8972 13.204 13.3625 12.6693C12.8277 12.1345 12.1784 11.8672 11.4146 11.8672C10.6507 11.8672 10.0014 12.1345 9.46663 12.6693C8.93191 13.204 8.66455 13.8533 8.66455 14.6172C8.66455 15.3811 8.93191 16.0304 9.46663 16.5651C10.0014 17.0998 10.6507 17.3672 11.4146 17.3672ZM6.83122 10.0339H13.2479C13.5076 10.0339 13.7255 9.94585 13.9015 9.76985C14.0769 9.59447 14.1646 9.37691 14.1646 9.11719V7.28385C14.1646 7.02413 14.0769 6.80627 13.9015 6.63027C13.7255 6.45488 13.5076 6.36719 13.2479 6.36719H6.83122C6.5715 6.36719 6.35363 6.45488 6.17763 6.63027C6.00225 6.80627 5.91455 7.02413 5.91455 7.28385V9.11719C5.91455 9.37691 6.00225 9.59447 6.17763 9.76985C6.35363 9.94585 6.5715 10.0339 6.83122 10.0339ZM4.99788 8.06302V18.2839V5.45052V8.06302Z" fill="white"/>
                                </svg>
                                {{ s24_int_m_generic_btn_save }}
                            </button>
                        </div>
                    </div>
                </div>
            </form>
        </div>

        <!-- Shipping methods -->
        <div class="tab-pane" id="tab-shipping-method">
            <div class="container-fluid">
                <div class="panel panel-default">
                    <div class="panel-heading heading-shipping-method">
                        <h3 class="panel-title">
                            {# <i class="fa fa-pencil"></i> #}
                            <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <path d="M3.99996 13.3334C3.4444 13.3334 2.97218 13.1389 2.58329 12.75C2.1944 12.3611 1.99996 11.8889 1.99996 11.3334H0.666626V2.66669H11.3333V5.33335H13.3333L15.3333 8.00002V11.3334H14C14 11.8889 13.8055 12.3611 13.4166 12.75C13.0277 13.1389 12.5555 13.3334 12 13.3334C11.4444 13.3334 10.9722 13.1389 10.5833 12.75C10.1944 12.3611 9.99996 11.8889 9.99996 11.3334H5.99996C5.99996 11.8889 5.80551 12.3611 5.41663 12.75C5.02774 13.1389 4.55552 13.3334 3.99996 13.3334ZM3.99996 12C4.18885 12 4.34729 11.936 4.47529 11.808C4.60285 11.6805 4.66663 11.5222 4.66663 11.3334C4.66663 11.1445 4.60285 10.9862 4.47529 10.8587C4.34729 10.7307 4.18885 10.6667 3.99996 10.6667C3.81107 10.6667 3.65263 10.7307 3.52463 10.8587C3.39707 10.9862 3.33329 11.1445 3.33329 11.3334C3.33329 11.5222 3.39707 11.6805 3.52463 11.808C3.65263 11.936 3.81107 12 3.99996 12ZM1.99996 10H2.53329C2.72218 9.80002 2.93885 9.63891 3.18329 9.51669C3.42774 9.39446 3.69996 9.33335 3.99996 9.33335C4.29996 9.33335 4.57218 9.39446 4.81663 9.51669C5.06107 9.63891 5.27774 9.80002 5.46663 10H9.99996V4.00002H1.99996V10ZM12 12C12.1888 12 12.3471 11.936 12.4746 11.808C12.6026 11.6805 12.6666 11.5222 12.6666 11.3334C12.6666 11.1445 12.6026 10.9862 12.4746 10.8587C12.3471 10.7307 12.1888 10.6667 12 10.6667C11.8111 10.6667 11.6528 10.7307 11.5253 10.8587C11.3973 10.9862 11.3333 11.1445 11.3333 11.3334C11.3333 11.5222 11.3973 11.6805 11.5253 11.808C11.6528 11.936 11.8111 12 12 12ZM11.3333 8.66669H14.1666L12.6666 6.66669H11.3333V8.66669Z" fill="#4C4D5A"/>
                            </svg>
                            {{ s24_int_m_title_shipping_method_settings }}
                        </h3>
                        <button class="btn btn-s24 btn-s24-primary add-shipping-method-btn"
                            data-btn-add-option
                            data-toggle="tooltip" title="{{ s24_int_m_tooltip_create }}"
                        >
                            <svg width="11" height="11" viewBox="0 0 11 11" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <path d="M4.35857 10.17V5.81143H0V4.35857H4.35857V0H5.81143V4.35857H10.17V5.81143H5.81143V10.17H4.35857Z" fill="white"/>
                            </svg>
                        </button>
                    </div>
                    <div class="panel-body">
                        <div class="table-responsive">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>{{ s24_int_m_sm_table_id }}</th>
                                        <th style="min-width: 180px;">{{ s24_int_m_sm_table_title }}</th>
                                        <th>{{ s24_int_m_sm_table_type }}</th>
                                        <th>{{ s24_int_m_sm_table_allowed_services }}</th>
                                        <th style="min-width: 120px;">{{ s24_int_m_sm_table_offer_priority }}</th>
                                        <th style="min-width: 100px;">{{ s24_int_m_sm_table_sort_order }}</th>
                                        <th colspan="2" style="min-width: 200px;">{{ s24_int_m_sm_table_enabled }}</th>
                                        {# <th>{{ s24_int_m_sm_table_actions }}</th> #}
                                    </tr>
                                </thead>
                                <tbody id="shipping_options">
                                    {{ s24_int_shipping_options }}
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Sender Settings -->
        {{ sender_tab_partial }}

        <!-- Available Services -->
        <div class="tab-pane" id="tab-services">
            <div class="container-fluid">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h3 class="panel-title"><i class="fa fa-pencil"></i>{{ s24_int_m_title_services_settings }}</h3>
                    </div>
                    <div class="panel-body">
                        <div class="table-responsive">
                            <table class="table table-striped table-hover">
                                <thead>
                                    <tr>
                                        {# <th>{{ s24_int_m_service_table_id }}</th> #}
                                        <th>{{ s24_int_m_service_table_code }}</th>
                                        <th>{{ s24_int_m_service_table_name }}</th>
                                        <th>{{ s24_int_m_service_table_type }}</th>
                                        <th>{{ s24_int_m_service_table_logo }}</th>
                                        <th>{{ s24_int_m_service_table_pickup }}</th>
                                        <th>{{ s24_int_m_service_table_delivery }}</th>
                                        <th>{{ s24_int_m_service_table_terminal_type }}</th>
                                        <th>{{ s24_int_m_service_table_additional_services }}</th>
                                    </tr>
                                </thead>
                                <tbody id="available_services">
                                {% for service in services %}
                                    <tr>
                                        {# <td>{{ service.get('id') }}</td> #}
                                        <td>{{ service.get('service_code') }}</td>
                                        <td>{{ service.get('name') }}</td>
                                        <td>{{ service.get('service_type') }}</td>
                                        <td><img height="30" src="{{ service.getImage() }}" alt=""></td>
                                        <td class="text-center">
                                            {% if service.get('pickup_from_address') %}
                                                <span class="service-enabled"></span>
                                            {% else %}
                                                <span class="service-disabled"></span>
                                            {% endif %}
                                        </td>
                                        <td class="text-center">
                                            {% if service.get('delivery_to_address') %}
                                                <span class="service-enabled"></span>
                                            {% else %}
                                                <span class="service-disabled"></span>
                                            {% endif %}
                                        </td>
                                        <td class="text-center">{{ service.get('additional') }}</td>
                                        <td class="additional-services">
                                            {% for additional_service, active in service.getAdditionalServices() %}
                                                <span class="service-{% if active %}enabled{% else %}disabled{% endif %}">{{ additional_service }}</span>
                                            {% endfor %}
                                        </td>
                                    </tr>
                                {% endfor %}
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Parcel Defaults -->
        <div class="tab-pane" id="tab-parcel-defaults">
            <div class="container-fluid">
                <div class="alert alert-warning"><i class="fa fa-exclamation-circle"></i>
                    {{ s24_int_m_description_parcel_defaults }}
                </div>

                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h3 class="panel-title">
